{
  "name": "@vcmap/export",
  "version": "2.1.1",
  "description": "Warehouse export plugin for VC Map",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "prepublishOnly": "vcmplugin build",
    "build": "vcmplugin build",
    "bundle": "vcmplugin bundle",
    "start": "vcmplugin serve",
    "preview": "vcmplugin preview",
    "buildStagingApp": "vcmplugin buildStagingApp",
    "lint:js": "eslint . --ext .vue,.js,.cjs,.mjs,.ts,.cts,.mts",
    "lint:prettier": "prettier --check .",
    "lint": "npm run lint:js && npm run lint:prettier",
    "format": "prettier --write --list-different . && npm run lint:js -- --fix",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "type-check": "vue-tsc --noEmit",
    "ensure-types": "vcmplugin ensure-types"
  },
  "author": "Tobias Krauth <tkrauth@vc.systems>",
  "license": "MIT",
  "keywords": [
    "vcmap",
    "plugin"
  ],
  "files": [
    "src/",
    "dist/",
    "plugin-assets/",
    "LICENSE.md",
    "README.md",
    "CHANGELOG.md"
  ],
  "exports": {
    ".": "dist/index.js",
    "./dist": "./dist/index.js"
  },
  "prettier": "@vcsuite/eslint-config/prettier.js",
  "peerDependencies": {
    "@vcmap-cesium/engine": "^11.0.2",
    "@vcmap/core": "^6.0.11",
    "@vcmap/ui": "^6.0.14",
    "ol": "^10.2.1",
    "vue": "~3.4.38",
    "vuetify": "^3.7.3"
  },
  "overrides": {
    "esbuild": "^0.25.0"
  },
  "devDependencies": {
    "@types/semver": "^7.7.0",
    "@vcmap/plugin-cli": "^4.1.3",
    "@vcsuite/eslint-config": "^4.0.0",
    "@vitest/coverage-v8": "^2.1.9",
    "jest-canvas-mock": "^2.5.2",
    "jsdom": "^24.0.0",
    "resize-observer-polyfill": "^1.5.1",
    "typescript": "^5.9.2",
    "vite-plugin-vuetify": "^2.0.3",
    "vitest": "^2.1.9",
    "vue-tsc": "^2.2.12"
  },
  "dependencies": {
    "@turf/boolean-disjoint": "~6.5.0",
    "@vcsuite/check": "^3.1.0",
    "@vcsuite/logger": "^1.0.1",
    "@vcsuite/parsers": "^2.0.1",
    "fast-deep-equal": "^3.1.3",
    "semver": "^7.7.2"
  },
  "mapVersion": "^6.0"
}

{
  "name": "@vcmap/export",
  "version": "2.0.0-rc.0",
  "description": "Warehouse export plugin for VC Map",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "prepublishOnly": "vcmplugin build",
    "build": "vcmplugin build",
    "pack": "vcmplugin pack",
    "start": "vcmplugin serve",
    "preview": "vcmplugin preview",
    "buildStagingApp": "vcmplugin buildStagingApp",
    "lint:js": "eslint . --ext .vue,.js,.cjs,.mjs,.ts,.cts,.mts",
    "lint:prettier": "prettier --check .",
    "lint": "npm run lint:js && npm run lint:prettier",
    "format": "prettier --write --list-different . && npm run lint:js -- --fix",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "author": "Tobias Krauth <tkrauth@vc.systems>",
  "license": "MIT",
  "keywords": [
    "vcmap",
    "plugin"
  ],
  "files": [
    "src/",
    "dist/",
    "plugin-assets/",
    "LICENSE.md",
    "README.md",
    "CHANGELOG.md"
  ],
  "exports": {
    ".": "./src/index.js",
    "./dist": "./dist/index.js"
  },
  "eslintIgnore": [
    "node_modules",
    "dist",
    "coverage",
    "plugin-assets"
  ],
  "eslintConfig": {
    "root": true,
    "extends": "@vcsuite/eslint-config/vue",
    "rules": {
      "jsdoc/check-tag-names": "off"
    }
  },
  "prettier": "@vcsuite/eslint-config/prettier.js",
  "dependencies": {
    "@turf/boolean-disjoint": "~6.5.0",
    "@vcsuite/check": "^1.1.2",
    "@vcsuite/logger": "^1.0.1",
    "@vcsuite/parsers": "^1.0.3",
    "fast-deep-equal": "^3.1.3",
    "semver": "^7.6.3"
  },
  "peerDependencies": {
    "@vcmap-cesium/engine": "^11.0.2",
    "@vcmap/core": "^6.0.0-rc.12",
    "@vcmap/ui": "^6.0.0-rc.10",
    "ol": "^10.2.0",
    "vue": "~3.4.38",
    "vuetify": "^3.7.2"
  },
  "devDependencies": {
    "@vcmap/plugin-cli": "^4.0.0-rc.4",
    "@vcsuite/eslint-config": "^3.0.7",
    "@vitest/coverage-v8": "^2.1.1",
    "jest-canvas-mock": "^2.5.2",
    "jsdom": "^24.0.0",
    "resize-observer-polyfill": "^1.5.1",
    "vitest": "^2.1.1"
  },
  "mapVersion": "^6.0"
}
